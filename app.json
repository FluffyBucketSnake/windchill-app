{
  "expo": {
    "name": "Wind Chill Calculator",
    "slug": "windchill-calculator",
    "version": "1.2.0",
    "entryPoint": "./src/App.tsx",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
